    "doctrine/migrations": "^3.7",
    "doctrine/orm": "^2.17",
    "firebase/php-jwt": "^6.10",
    "itsgoingd/clockwork": "^5.2",
    "league/flysystem": "^3.27",
    "league/flysystem-aws-s3-v3": "^3.27",
    "league/mime-type-detection": "^1.15",
    "php-di/php-di": "^7.0",
    "psr/simple-cache": "^3.0",
    "ramsey/uuid": "^4.7",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12",
    "slim/twig-view": "^3.3",
    "symfony/cache": "^7.0",
    "symfony/console": "^7.0",
    "symfony/mailer": "^7.0",
    "symfony/rate-limiter": "^7.0",
    "symfony/twig-bridge": "^7.0",
    "symfony/webpack-encore-bundle": "^2.1",
    "twig/intl-extra": "^3.9",
